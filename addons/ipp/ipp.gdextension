[configuration]

entry_symbol = "ipp_library_init"
compatibility_minimum = 4.2

[libraries]

macos.debug = "bin/libipp.macos.template_debug.framework"
macos.release = "bin/libipp.macos.template_release.framework"

windows.debug.x86_32 = "bin/ipp.windows.template_debug.x86_32.dll"
windows.release.x86_32 = "bin/ipp.windows.template_release.x86_32.dll"
windows.debug.x86_64 = "bin/ipp.windows.template_debug.x86_64.dll"
windows.release.x86_64 = "bin/ipp.windows.template_release.x86_64.dll"

linux.debug.x86_64 = "bin/libipp.linux.template_debug.x86_64.so"
linux.release.x86_64 = "bin/libipp.linux.template_release.x86_64.so"
linux.debug.arm64 = "bin/libipp.linux.template_debug.arm64.so"
linux.release.arm64 = "bin/libipp.linux.template_release.arm64.so"
linux.debug.rv64 = "bin/libipp.linux.template_debug.rv64.so"
linux.release.rv64 = "bin/libipp.linux.template_release.rv64.so"

